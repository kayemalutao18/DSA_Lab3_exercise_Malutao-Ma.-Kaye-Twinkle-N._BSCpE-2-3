def vowelsToUpper(input_string):
    vowels = 'aeiouAEIOU'
    return ''.join([char.upper() if char in vowels else char for char in input_string])

word1 = ("Happy computing")
word2 = ("data structures")

print(vowelsToUpper(""))  
print(word1 + " == " + vowelsToUpper("Happy computing"))  
# Output: HAppy cOmpUtIng
print(word2 + " == " + vowelsToUpper("data structures"))  
# Output: dAtA strUctUrEs
